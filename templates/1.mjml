                    </mj-column>

                    <mj-column padding="0" width="30px" css-class="col-40px">
                        <mj-image align="left" padding="0" width="30px"
                                  src="{{mainURL}}/static/mail-1/Images/telegram.png"></mj-image>
                    </mj-column>
                </mj-section>
            </mj-column>
            <mj-spacer css-class="spacer" height="40px"/>

        </mj-section>
    </mj-body>
</mjml>
