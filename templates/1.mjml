            </mj-column>
            <mj-column width="20%" css-class="col-50" vertical-align="bottom">
                <mj-text mj-class="footer-text" padding-bottom="5px">
                    Пишите<br/>или звоните:
                </mj-text>
                <mj-spacer css-class="spacer__mini" height="20px"/>
                <mj-section padding="0" text-align="left" css-class="footer-block__left">
                    <mj-column padding="0 10px 0 0" width="40px" css-class="col-40px">
                        <mj-image align="left" padding="0" width="30px"
                                  src="{{mainURL}}/static/mail-1/Images/whatsapp.png">
