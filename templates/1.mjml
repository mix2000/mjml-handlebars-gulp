<mjml>
    <mj-head>
        <mj-font name="Montserrat"
                 href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700&display=swap"/>

        <mj-attributes>
            <mj-class name="header" padding-top="0" padding-bottom="0" font-size="30px" align="center" font-weight="500"
                      color="#000000" font-family="Montserrat"/>
            <mj-class name="sub-header" padding-top="0" padding-bottom="0" font-size="20px" align="center"
                      font-weight="400"
                      color="#000000" font-family="Montserrat"/>
            <mj-class name="content" color="black" font-size="16px" line-height="21px" padding-top="0" width="510px"
                      padding-bottom="0"/>
            <mj-class name="button" font-family="Montserrat" font-size="15px" padding="0" border-radius="10px"
                      width="306px" inner-padding="14px 0" text-transform="uppercase" background-color="#8D0808"
                      color="#FFEFEF"/>
            <mj-class name="footer-text" color="#ffffff" font-size="13px" padding="0" font-family="Montserrat"/>
            <mj-class name="footer-text__bold" font-weight="600"/>
        </mj-attributes>
        <mj-style>
            @media (min-width: 481px) {
                .image-mobile {
                    display: none !important;
                }
            }

            @media (max-width: 480px) {
                .header > div {
                    font-size: 17px !important;
                }

                .content > div {
                    font-size: 13px !important;
                }

                .mj-column-per-33 {
                    width: 50% !important;
                }

                .button > table, .button p {
                    width: 248px !important;
                }

                .button p {
                    font-size: 12px !important;
                }

                .sub-header > div {
                    font-size: 15px !important;
                }

                .image-desktop {
                    display: none;
                }

                .spacer > div {
                    height: 16px !important;
                }

                .spacer__mini > div {
                    height: 8px !important;
                }

                .main-logo img, .main-logo td {
                    width: 264px !important;
                }

                .col-50 {
                    width: 50% !important;
                    padding-top: 20px !important;
                }

                .col-40px {
                    width: 40px !important;
                }

                .footer-block__left {
                    margin: 0 !important;
                }
            }
        </mj-style>
    </mj-head>
    <mj-body>
        <mj-section padding-bottom="0">
            <mj-column>
                <mj-image width="430px" css-class="main-logo" align="center" padding-bottom="0" src="{{mainURL}}/static/mail-1/Images/logo_light.png"></mj-image>
                <mj-spacer css-class="spacer" height="40px"/>

                <mj-text mj-class="header" css-class="header">Поставки техники
                    <span style="color:#8D0808; font-weight: 700;">Neff</span>
                </mj-text>
                <mj-spacer css-class="spacer" height="40px"/>
                <mj-text mj-class="content" css-class="content" font-weight="bold">Коллеги, доброго дня!</mj-text>
                <mj-text mj-class="content" css-class="content">Мы являемся импортёром бытовой техники
                    <span style="color:#8D0808; font-weight: 700;">Neff</span>
                    в Россию.
                    <br/>
                    Мы поставляем русифицированную технику
                    <span style="color:#8D0808; font-weight: 700;">Neff</span>
                    из Европы на
                    <span style="font-weight: 700;">оптовых условиях</span>
                    для дилеров по всей
                    России.
                </mj-text>
            </mj-column>
        </mj-section>
        <mj-section padding-bottom="0">
            <mj-column>
                <mj-image css-class="image-desktop" padding="0" src="{{mainURL}}/static/mail-1/Images/001.png"></mj-image>
                <mj-image css-class="image-mobile" width="200px" padding="0" src="{{mainURL}}/static/mail-1/Images/m_001.png"></mj-image>
                <mj-spacer height="20px"/>
                <mj-image css-class="image-desktop" padding="0" src="{{mainURL}}/static/mail-1/Images/002.png"></mj-image>
                <mj-image css-class="image-mobile" width="200px" padding="0" src="{{mainURL}}/static/mail-1/Images/m_002.png"></mj-image>
                <mj-spacer height="20px"/>
                <mj-image css-class="image-desktop" padding="0" src="{{mainURL}}/static/mail-1/Images/003.png"></mj-image>
                <mj-image css-class="image-mobile" width="200px" padding="0" src="{{mainURL}}/static/mail-1/Images/m_003.png"></mj-image>
                <mj-spacer height="20px"/>
                <mj-image css-class="image-desktop" padding="0" src="{{mainURL}}/static/mail-1/Images/004.png"></mj-image>
                <mj-image css-class="image-mobile" width="200px" padding="0" src="{{mainURL}}/static/mail-1/Images/m_004.png"></mj-image>
                <mj-spacer height="20px"/>
                <mj-image css-class="image-desktop" padding="0" src="{{mainURL}}/static/mail-1/Images/005.png"></mj-image>
                <mj-image css-class="image-mobile" width="200px" padding="0" src="{{mainURL}}/static/mail-1/Images/m_005.png"></mj-image>
                <mj-spacer height="20px"/>
                <mj-image css-class="image-desktop" padding="0" src="{{mainURL}}/static/mail-1/Images/006.png"></mj-image>
                <mj-image css-class="image-mobile" width="200px" padding="0" src="{{mainURL}}/static/mail-1/Images/m_006.png"></mj-image>
                <mj-spacer height="20px"/>
                <mj-image css-class="image-desktop" padding="0" src="{{mainURL}}/static/mail-1/Images/007.png"></mj-image>
                <mj-image css-class="image-mobile" width="200px" padding="0" src="{{mainURL}}/static/mail-1/Images/m_007.png"></mj-image>
                <mj-spacer css-class="spacer" height="40px"/>
                <mj-button mj-class="button" css-class="button">
                    Получить прайсы и остатки
                </mj-button>
                <mj-spacer css-class="spacer" height="40px"/>
                <mj-text mj-class="sub-header" css-class="sub-header" padding-bottom="16px">
